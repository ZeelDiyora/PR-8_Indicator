#include<stdio.h>

int main(){
    int i,j,k,sum=0;
    
    printf("Enter array size : ");
    scanf("%d",&i);
    int a[i][i];
    
    printf("Enter the elements of array : ");
    for(j=0;j<i;j++)
    {
        for(k=0;k<i;k++)
      {
        printf("a[%d][%d]=",j,k);
        scanf("%d",&a[j][k]);
      }
    }
    for(j=0;j<k;j++)
    {
        for(k=0;k<i;k++)
      {
            
        a[j][k] = a[j][k] * a[j][k] * a[j][k];
        
        for(k=0;k<j;k++)
        {
        printf("a[%d]=%d\n",k,a[k]);
        }
        
      }
    }
  return 0;
}
